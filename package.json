{
	"name": "vscode-query-runner",
	"displayName": "vscode-query-runner",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openQueryRunner"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openQueryRunner",
				"title": "QueryRunner: Open QueryRunner"
			}
		],
		"configuration": {
      "type": "object",
      "title": "QueryRunner extension configuration",
      "properties": {
        "queryRunner.keyFilename": {
          "type": "string",
          "default": "",
          "description": "Full path to the a .json, .pem, or .p12 key downloaded from the Google Developers Console. NOTE: .pem and .p12 require you to specify the email option as well."
        },
        "queryRunner.projectId": {
          "type": "string",
          "default": "",
          "description": "The project ID from the Google Developer's Console, e.g. 'grape-spaceship-123'."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@google-cloud/bigquery": "^4.1.6",
		"@types/flat": "0.0.28",
		"flat": "^4.1.0",
		"vue": "^2.6.10"
	}
}
